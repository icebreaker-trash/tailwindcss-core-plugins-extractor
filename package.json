    "jest": "^29.5.0",
    "package-json": "^8.1.0",
    "rollup": "^3.21.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "semver": "^7.3.5",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "ts-patch": "^2.1.0",
    "tslib": "^2.3.1",
